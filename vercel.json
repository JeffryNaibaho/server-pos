{
    "version": 2,
    "framework": null,
    "functions": {
        "api/*.php": { "runtime": "vercel-php@0.7.2" }
    },
    "routes": [
        {
            "src": "/(favicon\\.ico|robots\\.txt)",
            "dest": "/public/$1"
        },
        {
            "src": "/(css|js|images|storage)/(.*)",
            "dest": "/public/$1/$2"
        },
        {
            "src": "/api/cek.php",
            "dest": "/api/cek.php"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "true",
        "APP_URL": "https://server-pos-blue.vercel.app"
    }
}